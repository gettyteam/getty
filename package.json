  "description": "The platform tools for live streaming on Odysee. This includes overlays, tip alerts, chat, giveaway system, creator analytics and much more.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "dev": "npm-run-all copy:primeicons copy:landing --parallel dev-server dev-css",
    "dev-server": "nodemon --config nodemon.json",
    "admin:dev": "vite --config admin-frontend/vite.config.js",
    "admin:build": "vite build --config admin-frontend/vite.config.js",
    "frontend:dev": "vite --config frontend/vite.config.js",
    "frontend:build": "vite build --config frontend/vite.config.js",
    "dev-css": "tailwindcss -i ./src/input.css -o ./public/css/output.css --watch",
    "test": "cross-env NODE_ENV=test node scripts/run-tests.js",
    "lint:css": "stylelint \"src/**/*.css\" --cache",
    "lint:css:fix": "stylelint \"src/**/*.css\" --fix --cache",
    "build-css": "tailwindcss -i src/input.css -o public/css/output.css",
    "watch-css": "tailwindcss -i src/input.css -o public/css/output.css --watch",
    "minify-js": "esbuild public/js/*.js --minify --outdir=public/js/min",
    "minify-css": "postcss public/css/output.css -o public/css/output.css --config postcss.config.cjs",
    "build:i18n": "node scripts/build-i18n.js",
    "i18n:audit:dashboard": "node scripts/audit-i18n-dashboard.js",
    "build-dashboard": "esbuild src/index.js --bundle --format=cjs --outfile=public/js/dashboard.js",
    "build": "npm-run-all --sequential copy:primeicons copy:landing build-css minify-css build:i18n build-dashboard admin:build frontend:build sync:frontend minify-js add-sri",
    "build:secure": "infisical run --env=prod -- npm run build",
    "build:render": "cross-env BUILD_LIGHT=1 npm-run-all --sequential copy:primeicons copy:landing build-css minify-css build:i18n frontend:build sync:frontend minify-js add-sri",
    "add-sri": "node scripts/add-sri.js",
    "copy:primeicons": "node scripts/copy-primeicons.js",
    "copy:landing": "node scripts/copy-landing-assets.js",
    "sync:frontend": "node scripts/sync-frontend-dist.js",
    "lint": "eslint admin-frontend/src frontend/src modules routes scripts server.js --ext .js,.vue --cache --max-warnings=0",
    "lint:fix": "eslint admin-frontend/src frontend/src modules routes scripts server.js --ext .js,.vue --fix --cache",
    "report:unused-css": "node scripts/unused-css-report.js",
    "verify:hosted": "node scripts/hosted-verify.js",
    "db:migrate": "node scripts/db-run-migrations.js",
    "db:import-history": "node scripts/import-stream-history.js",
    "db:import-history:bulk": "node scripts/import-stream-history-bulk.js"
  },
