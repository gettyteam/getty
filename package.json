    "react-aria-components": "^1.13.0",
    "shadcn-vue": "^2.3.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-react-aria-components": "^1.0.0",
    "vue-chartjs": "^5.3.3",
    "vue-i18n": "^11.2.1",
    "vue-router": "^4.6.3",
    "ws": "8.18.3",
    "zod": "^4.1.13"
